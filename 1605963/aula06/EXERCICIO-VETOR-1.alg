algoritmo "EXERCICIO-VETOR-1"

var

VEICULOS: VETOR[0..4] DE CARACTERE
CONT: INTEIRO


inicio

CONT <- 0
ENQUANTO (CONT < 5) FACA

   ESCREVAL("DIGITE O VEICULO:")
   LEIA(VEICULOS[CONT])
   
   CONT <- CONT + 1
FIMENQUANTO

LIMPATELA

ESCREVAL("OS VEICULOS CADASTRADOS FORAM:")
CONT <- 0
ENQUANTO (CONT < 5) FACA
  ESCREVAL(" - ", VEICULOS[CONT])
  
  CONT <- CONT + 1
FIMENQUANTO

fimalgoritmo
