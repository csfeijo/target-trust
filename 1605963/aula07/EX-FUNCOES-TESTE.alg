algoritmo "semnome"

var

FUNCAO MEDIA(NOTA1, NOTA2: INTEIRO):CARACTERE
VAR
MEDIA: REAL
TEXTO: CARACTERE
INICIO
  MEDIA <- (NOTA1 + NOTA2) / 2

  SE (MEDIA > 7) ENTAO
     TEXTO <- "VOCE FOI APROVADO!"
  SENAO
     TEXTO <- "VOCE FOI REPROVADO!"
  FIMSE

  RETORNE TEXTO
FIMFUNCAO

inicio

ESCREVAL( MEDIA(10, 10) )




fimalgoritmo
