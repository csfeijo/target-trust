algoritmo "CAP8-EX-3"


var
IDADE: INTEIRO
CONT: INTEIRO
SOMA: INTEIRO
MAX: INTEIRO

inicio
MAX <- 5

CONT <- 1
ENQUANTO (CONT <= MAX) FACA
   ESCREVAL("DIGITE A IDADE DA PESSOA:")
   LEIA(IDADE)
   
   SOMA <- SOMA + IDADE
   
   CONT <- CONT + 1
FIMENQUANTO

ESCREVAL("A SOMA DAS IDADES É: ", SOMA)


fimalgoritmo
