algoritmo "CAP5-EX4"

var
CARGA_HORARIA_MENSAL: INTEIRO
SALARIO_FIXO: REAL
VALOR_HORA: REAL

inicio

ESCREVAL("QUAL SUA CARGA HORÁRIA MENSAL?")
LEIA(CARGA_HORARIA_MENSAL)

ESCREVAL("QUAL O SEU SALARIO FIXO?")
LEIA(SALARIO_FIXO)

VALOR_HORA <- SALARIO_FIXO / CARGA_HORARIA_MENSAL)

ESCREVAL("VOCE RECEBE POR HORA: $", VALOR_HORA)


fimalgoritmo
