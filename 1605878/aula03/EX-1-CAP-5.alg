algoritmo "EX-1-CAP-5"

var
NUM1: REAL
NUM2: REAL


inicio

ESCREVAL("DIGITE O PRIMEIRO NUMERO")
LEIA(NUM1)

ESCREVAL("DIGITE O SEGUNDO NUMERO")
LEIA(NUM2)

ESCREVAL("SOMA:", NUM1 + NUM2)
ESCREVAL("SUBTRAÇÃO:", NUM1 - NUM2)
ESCREVAL("MULTIPLICAÇÃO:", NUM1 * NUM2)
ESCREVAL("DIVISÃO", NUM1 / NUM2)







fimalgoritmo
